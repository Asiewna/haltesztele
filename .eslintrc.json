{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["prettier", "react"],
  "rules": {
    "no-use-before-define": 0,
    "react/jsx-filename-extension": 0,
    "class-methods-use-this": 0,
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "no-underscore-dangle": 0
  }
}
